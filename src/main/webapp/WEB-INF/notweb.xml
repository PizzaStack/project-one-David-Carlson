<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="3.0">
  <servlet>
    <servlet-name>helloServlet</servlet-name>
    <servlet-class>com.revature.servlets.HelloServlet</servlet-class>
    <init-param>
      <param-name>user</param-name>
      <param-value>HelloPadawan</param-value>
    </init-param>
  </servlet>
  <welcome-file-list>
    <welcome-file>test.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>FanficServlet</display-name>
    <servlet-name>FanficServlet</servlet-name>
    <servlet-class>com.revature.servlets.FanficServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FanficServlet</servlet-name>
    <url-pattern>/FanficServlet</url-pattern>
  </servlet-mapping>
  <error-page>
    <error-code>404</error-code>
    <location>/404.html</location>
  </error-page>
  <context-param>
    <param-name>user</param-name>
    <param-value>Padawan</param-value>
  </context-param>
  <servlet>
    <description></description>
    <display-name>DatabaseMetaServlet</display-name>
    <servlet-name>DatabaseMetaServlet</servlet-name>
    <servlet-class>com.revature.servlets.DatabaseMetaServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DatabaseMetaServlet</servlet-name>
    <url-pattern>/DatabaseMetaServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ConnectionServlet</display-name>
    <servlet-name>ConnectionServlet</servlet-name>
    <servlet-class>com.revature.servlets.ConnectionServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ConnectionServlet</servlet-name>
    <url-pattern>/ConnectionServlet</url-pattern>
  </servlet-mapping>
</web-app>